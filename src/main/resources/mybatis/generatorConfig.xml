<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" [

  <!ENTITY connector "mysql-connector-java-5.1.32.jar">
  <!ENTITY dbName "tmpay">
  <!ENTITY dbHost "172.18.76.175">
  <!ENTITY dbUsername "lamiccn">
  <!ENTITY dbPassword "zy9199Hltx1!2@LamicRDS">
  <!ENTITY mysqlDir "D:\java\apache-maven-3.5.0\repository\mysql\mysql-connector-java\5.1.32">
  <!ENTITY mysqlJAR "&mysqlDir;/&connector;">
  
  <!ENTITY baseTarget "D:\workspace\idea\lamic-pay-new">
  <!ENTITY targetPrefix "\lamic-pay-service-trade-api\">
  <!ENTITY targetPrefixService "\lamic-pay-service-trade\">
  <!ENTITY target "src/main/java">
  <!ENTITY modelTargetPackage "com.hltx.lamic.pay.entity">
]>
<generatorConfiguration>
  <classPathEntry location="&mysqlJAR;" />
    <!-- 修改defaultModelType是因为payorder表paytime也是主键，flat不生成主键实体 -->
  <context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="flat">
	
	<!-- beginningDelimiter和endingDelimiter：指明数据库的用于标记数据库对象名的符号，比如ORACLE就是双引号，MYSQL默认是`反引号； -->
	<property name="beginningDelimiter" value="`"/>
    <property name="endingDelimiter" value="`"/>
	<!-- 当表名或者字段名为SQL关键字的时候，可以设置该属性为true，MBG会自动给表名或字段名添加`分隔符 -->
	<property name="autoDelimitKeywords" value="true"/>
	
	
	<plugin type="org.mybatis.generator.plugins.SerializablePlugin" /> 
	
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://&dbHost;:3306/&dbName;"
        userId="&dbUsername;"
        password="&dbPassword;">
    </jdbcConnection>
	
    <javaModelGenerator targetPackage="&modelTargetPackage;" targetProject="&baseTarget;&targetPrefix;&target;">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>
	
    <sqlMapGenerator targetPackage="com.hltx.lamic.pay.persistence"  targetProject="&baseTarget;&targetPrefixService;&target;">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.hltx.lamic.pay.persistence"  targetProject="&baseTarget;&targetPrefixService;&target;">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

   
   <!-- schema即为数据库名, tableName为对应的数据库表, domainObjectName是要生成的实体类  -->
	
	
   <table schema="&dbName;" tableName="payorder" domainObjectName="Payorder" enableCountByExample="true" enableUpdateByExample="false" enableDeleteByExample="false"
        enableSelectByExample="true" selectByExampleQueryId="false">
      <property name="useActualColumnNames" value="false"/>
      <generatedKey column="id" sqlStatement="SELECT UUID()" identity="true" />
   </table>
  
   <table schema="&dbName;" tableName="extrapar_payorder" domainObjectName="ExtraparPayorder" enableCountByExample="true" enableUpdateByExample="false" enableDeleteByExample="false"
        enableSelectByExample="true" selectByExampleQueryId="false">
      <property name="useActualColumnNames" value="false"/>
      <!--<generatedKey column="id" sqlStatement="MySql" identity="true" />-->
   </table>
  </context>
</generatorConfiguration>
